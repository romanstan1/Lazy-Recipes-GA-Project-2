  <br/>
  <br/>
  <br/>
  <div class="demographic">
    <div>
      <span>Gender:</span> <%= gender %>
    </div>
  </div>
  <div class="demographic">
    <div>
      <span>Age Group:</span> <%= ageGroup %>
